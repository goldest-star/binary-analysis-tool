{
  "name": "hobbits",
  "version": "0.20.4",
  "private": true,
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {}
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "echo $HUSKY_GIT_STDIN | git-lfs-hooks/post-checkout $HUSKY_GIT_PARAMS",
      "post-commit": "echo $HUSKY_GIT_STDIN | git-lfs-hooks/post-commit $HUSKY_GIT_PARAMS",
      "post-merge": "echo $HUSKY_GIT_STDIN | git-lfs-hooks/post-merge $HUSKY_GIT_PARAMS",
      "pre-push": "echo $HUSKY_GIT_STDIN | git-lfs-hooks/pre-push $HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "7zip-bin": "^5.0.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.3",
    "@semantic-release/npm": "^7.0.3",
    "@semantic-release/release-notes-generator": "^9.0.0",
    "commitizen": "^4.0.3",
    "del": "^5.1.0",
    "filecompare": "^1.0.4",
    "glob": "^7.1.6",
    "husky": "^4.2.3",
    "node-7z": "^2.1.1",
    "semantic-release": "^17.0.3",
    "yargs": "^15.1.0"
  }
}
